{
  "name": "palm",
  "version": "0.0.1",
  "packageManager": "npm@10.2.3",
  "scripts": {
    "dev": "npm-run-all --parallel dev:kernel dev:tauri",
    "dev:tauri": "cd src-tauri/app && tauri dev",
    "dev:all": "npm-run-all --parallel dev:index dev:kernel dev:markdown dev:database",
    "dev:index": "cd src-ui/index && pnpm dev --port=1420",
    "dev:kernel": "cd src-ui/kernel && pnpm dev --port=1420",
    "dev:markdown": "cd src-ui/markdown && pnpm dev --port=1430",
    "dev:database": "cd src-ui/database && pnpm dev --port=1420",
    "build:index": "cd src-ui/index && pnpm build",
    "build:kernel": "cd src-ui/kernel && pnpm build",
    "build:markdown": "cd src-ui/markdown && pnpm build",
    "build:database": "cd src-ui/database && pnpm build",
    "build:all": "pnpm rimraf ./dist && npm-run-all --parallel build:index build:kernel build:markdown build:database",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:release": "cd src-tauri/app && tauri build",
    "lezer-generator": "lezer-generator src-ui/plugins/lang-sql/sql.grammar -o src-ui/plugins/lang-sql/sql.grammar.js"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@highlightjs/vue-plugin": "^2.1.2",
    "@tauri-apps/api": "^1.5.3",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "ant-design-vue": "~4.0.8",
    "dayjs": "^1.11.10",
    "pinia": "^2.1.7",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.4.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@tauri-apps/cli": "^1.5.9",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sass": "^1.69.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-html": "^3.2.1",
    "vue-tsc": "^1.8.27",
    "web-vitals": "^3.5.1"
  },
  "workspaces": [
    "./src-ui/*"
  ]
}
